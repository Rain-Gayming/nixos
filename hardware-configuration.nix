    { device = "/dev/disk/by-uuid/47D1-6BCB";
      fsType = "vfat";
      options = [ "fmask=0077" "dmask=0077" ];
    };

  swapDevices =
    [ { device = "/dev/disk/by-uuid/6642e848-2fe0-48b4-ad62-1d5d3359c37d"; }
